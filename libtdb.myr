use std

pkg libtdb =
	const newid : (-> byte[40])
	const readtdbmeta : (path : byte[:] -> std.result(std.htab(byte[:], byte[:])#, (int, byte[:])))
;;

const newid = {
	var bytes : byte[32]
	var id : byte[40]
	
	std.randbytes(bytes[:])
	std.bfmt(id[:], "{r}", id)
	-> id
}

const readtdbmeta = {path
	var metadata
	
	metadata = std.mkht(std.slhash, std.sleq)
	
	-> `std.Ok metadata
}
